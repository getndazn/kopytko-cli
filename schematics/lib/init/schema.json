{
  "$schema": "http://json-schema.org/schema",
  "$id": "SchematicsKopytkoInit",
  "title": "Kopytko Init Schema",
  "type": "object",
  "properties": {
    "rokuIP": {
      "type": "string",
      "default": "",
      "x-prompt": "What is your Roku device IP?"
    },
    "testingFramework": {
      "type": "boolean",
      "default": true,
      "x-prompt": "Would you like to use kopytko-unit-testing-framework?"
    }
  }
}
